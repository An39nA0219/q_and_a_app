<div class="wrapper">
  <div class="container">
    <%= render 'layouts/tabs', active: 'all' %>
    <%= render 'layouts/search_field', url: '/questions' %>
    <% @questions.each do |question|%>
      <%= render 'layouts/question', question: question %>
    <% end %>
    <%= render 'layouts/paginate', questions: @questions %>
  </div>
</div>