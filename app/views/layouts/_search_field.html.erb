<div class="search-area">
  <%= form_with url: url, method: :get do |f| %>
    <div class="input-group">
      <%= f.text_field :words, class: 'form-control' %>
      <span class="input-group-append">
        <%= f.submit '検索', class: 'btn btn-primary'%> 
      </span>
    </div>
  <% end %>
</div>
