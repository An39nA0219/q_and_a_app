<div class="wrapper">
  <div class="container">
    <%= render 'layouts/admins_tabs', active: 'all' %>
    <%= render 'layouts/search_field', url: '/admins/questions' %>
    
    <% @questions.each do |question|%>
      <%= render 'layouts/question', question: question, show_path: admins_question_path(question.id) %>
    <% end %>
    <%= render 'layouts/paginate', questions: @questions %>
  </div>
</div>